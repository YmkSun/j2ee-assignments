<?xml version="1.0" encoding="UTF-8"?>

<web-app>

<display-name>Repeat Visitor</display-name>

<servlet>
	<servlet-name>loginUser</servlet-name>
	<servlet-class>repeatVisit.RepeatVisitServlet</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>loginUser</servlet-name>
	<url-pattern>/login</url-pattern>
