<?xml version="1.0" encoding="UTF-8"?>

<web-app>

<display-name>Customer Food Order</display-name>

<servlet>
	<servlet-name>Display</servlet-name>
	<servlet-class>foodOrder.DisplayServlet</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>Display</servlet-name>
	<url-pattern>/display</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>loginAccept</servlet-name>
	<servlet-class>foodOrder.AcceptServlet</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>loginAccept</servlet-name>
	<url-pattern>/login</url-pattern>
</servlet-mapping>

<welcome-file-list>
	<welcome-file>login.html</welcome-file>
</welcome-file-list>

</web-app>
